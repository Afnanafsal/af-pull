{
  "name": "af-pull",
  "displayName": "AF-Pull",
  "description": "A smart Git auto-pull & commit assistant by Afnan Afsal. Keeps your workspace updated, prevents conflicts, and handles pull-rebase before commit. Auto detects upstream changes to currently edited files.",
  "version": "1.3.0",
  "publisher": "afnanafsal",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "SCM Providers",
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "afpull.pullNow",
        "title": "AF-Pull: Pull Now"
      },
      {
        "command": "afpull.commitPushWithPull",
        "title": "AF-Pull: Commit & Push (Pull First)"
      },
      {
        "command": "afpull.checkStatus",
        "title": "AF-Pull: Check Git Status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.0.0",
    "@types/play-sound": "^1.1.2",
    "@types/vscode": "^1.70.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afnanafsal/af-pull"
  },
  "bugs": {
    "url": "https://github.com/afnanafsal/af-pull/issues"
  },
  "homepage": "https://github.com/afnanafsal/af-pull#readme",
  "keywords": [
    "git",
    "auto-pull",
    "afnanafsal",
    "afnan",
    "af-pull",
    "git-pull",
    "git-auto-pull",
    "auto commit",
    "pull-rebase",
    "conflict-check",
    "live file monitor",
    "status bar git",
    "git protection",
    "vscode-extension",
    "afpull",
    "bot",
    "team-code",
    "git sync"
  ],
  "icon": "media/icon.png",
  "license": "MIT",
  "dependencies": {
    "play-sound": "^1.1.6"
  }
}
